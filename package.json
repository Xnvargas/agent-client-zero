{
  "name": "@kuntur/a2a-carbon-chat-adapter",
  "version": "0.1.14",
  "description": "A2A protocol adapter for Carbon AI Chat - connect any A2A agent to Carbon Chat UI",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server.d.ts",
        "default": "./dist/server.js"
      },
      "require": {
        "types": "./dist/server.d.cts",
        "default": "./dist/server.cjs"
      }
    },
    "./styles": "./dist/styles/index.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "@carbon/ai-chat": ">=1.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "@carbon/ai-chat": {
      "optional": false
    }
  },
  "devDependencies": {
    "@carbon/ai-chat": "^1.5.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.57.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "a2a",
    "agent-to-agent",
    "carbon",
    "ai-chat",
    "streaming",
    "llm",
    "kuntur",
    "ibm",
    "watson"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Xnvargas/a2a-carbon-chat-adapter.git"
  },
  "bugs": {
    "url": "https://github.com/Xnvargas/a2a-carbon-chat-adapter/issues"
  },
  "homepage": "https://github.com/Xnvargas/a2a-carbon-chat-adapter#readme",
  "license": "MIT",
  "author": "Xavier Vargas"
}
